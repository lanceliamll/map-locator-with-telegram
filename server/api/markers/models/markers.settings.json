      "type": "string"
    },
    "data": {
      "type": "json"
    },
    "symptoms": {
      "type": "json"
    },
    "mobileNumber": {
      "type": "string"
    },
    "severe": {
      "type": "boolean",
      "default": false
    },
    "common": {
      "type": "boolean",
      "default": false
    },
    "uncommon": {
      "type": "boolean",
      "default": false
